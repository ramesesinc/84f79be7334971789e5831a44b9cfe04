import com.rameses.annotations.*;

class LbpPaymentWebhookInterceptor {
	
	@After(pattern="CloudPaymentService.postPartnerWebhook", eval="#{args[0].paypartnerid.toString().equalsIgnoreCase('lbp')}")
	public void afterPostPartnerWebhook(evt) { 
		def result = evt.result;
		def param = evt.args[0];
		def data = param.data;

		println '\n** afterPostPartnerWebhook'
		param.each{
			println '> '+ it; 
		}
	}
}
